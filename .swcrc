{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": true
    },
    "target": "es2022",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}
