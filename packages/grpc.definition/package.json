{
  "name": "@gymlabs/core.grpc.definition",
  "version": "0.0.0",
  "author": "GymLabs",
  "description": "GymLabs Core gRPC types",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretypes": "rm -rf src/types",
    "types": "proto-loader-gen-types --outDir=src/types/ --grpcLib=@grpc/grpc-js --defaults src/service.proto",
    "build": "tsup src"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6"
  }
}
