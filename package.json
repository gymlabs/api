{
  "name": "@gymlabs/core",
  "version": "0.0.0",
  "author": "GymLabs",
  "description": "Core API module for GymLabs",
  "scripts": {
    "prepare": "husky install",
    "dev": "lerna run dev",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "lint:fix": "lerna run \"lint:fix\"",
    "format": "lerna run format",
    "format:check": "lerna run \"format:check\""
  },
  "lint-staged": {
    "src/**/*.ts": [
      "lerna run \"lint:fix\"",
      "lerna run format"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lerna": "^7.0.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@changesets/cli": "^2.26.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
