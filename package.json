{
  "name": "core.api",
  "version": "0.0.1",
  "description": "Core module for GymLabs architecture",
  "main": "main.js",
  "scripts": {
    "dev": "dotenv -e .env -- concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w -d build",
    "watch-dev": "nodemon --watch \"build/**/*\" -e js ./build/main.js | pino-pretty",
    "build": "swc src -d build",
    "start": "NODE_ENV=production node ./build/main.js",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "format": "prettier --write \"src/**/*\"",
    "prisma:generate": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymlabs/core.api.git"
  },
  "author": "GymLabs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gymlabs/core.api/issues"
  },
  "homepage": "https://github.com/gymlabs/core.api#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.46",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "prisma-dbml-generator": "^0.10.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-errors": "^3.11.0",
    "@pothos/plugin-prisma": "^3.47.1",
    "@pothos/plugin-relay": "^3.39.0",
    "@pothos/plugin-scope-auth": "^3.19.0",
    "@pothos/plugin-simple-objects": "^3.6.7",
    "@pothos/plugin-validation": "^3.9.0",
    "@pothos/plugin-with-input": "^3.10.0",
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "^3.8.0",
    "pino": "^8.11.0",
    "type-fest": "^3.7.2",
    "zod": "^3.21.4"
  }
}
